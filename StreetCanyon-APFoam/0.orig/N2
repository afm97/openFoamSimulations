FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object       N2;
} 
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * // 
 
dimensions      [0 0 0 0 0 0 0]; 
 
 
internalField   uniform  0.763364;
 
boundaryField 
{ 

    inlet
    {
        type            fixedValue; 
        value           $internalField;
    }
    
    outlet
    {
        type            inletOutlet; 
        inletValue      $internalField;
        value           $internalField;
    }
    
    topWall
    {
        type            zeroGradient;
    }

    frontAndBack
    {
	type            empty;
    }
    
    building
    {
        type            zeroGradient;
    }
}


